{
  "name": "@wadiwaw/wadiwaw-core",
  "version": "0.6.9",
  "description": "Wadiwaw UI Component",
  "repository": "https://github.com/akbarnafisa/vue-monorepo-boilerplate.git",
  "scripts": {
    "serve": "vue-cli-service serve --port 8181",
    "build": "rm -rf dist &&  rollup -c",
    "prepublish": "yarn build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index-es.js",
  "files": [
    "src/assets",
    "src/components",
    "src/styles",
    "src/utils",
    "src/globalComponents.js",
    "index.js",
    "index-es.js",
    "dist",
    "intellisense"
  ],
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "sideEffects": false,
  "vetur": {
    "tags": "./dist/intellisense/tags.json",
    "attributes": "./dist/intellisense/attributes.json"
  }
}

{
	"FAutocomplete default": {
		"prefix": "fautoc",
		"body": [
			"<f-autocomplete",
			"  v-model=\"${1:data}\"",
			"  :options=\"${2:options}\"",
			"  label=\"${3:Label}\"",
			"/>",
			""
		],
		"description": "FAutocomplete default"
	},
	"FAutocomplete with custom slot": {
		"prefix": "fautoc-slot",
		"body": [
			"<f-autocomplete",
			"  v-model=\"${1:data}\"",
			"  :options=\"${2:options}\"",
			"  label=\"${3:Label}\"",
			">",
			"  <template",
			"    slot=\"item\"",
			"    slot-scope=\"option\"",
			"  >",
			"    <div",
			"      class=\"f-body body-16 py-8 px-16\"",
			"      v-html=\"option.title\"",
			"    ></div>",
			"  </template>",
			"</f-autocomplete>",
			""
		],
		"description": "FAutocomplete with custom slot"
	},
	"FBtn default": {
		"prefix": "fbtn",
		"body": [
			"<f-btn type=\"${1:primary}\" size=\"${2:large}\">",
			" ${3:Text}",
			"</f-btn>",
			""
		],
		"description": "FBtn default"
	},
	"FBtn with icon": {
		"prefix": "fbtn-icon",
		"body": [
			"<f-btn type=\"${1:primary}\" size=\"${2:large}\" icon>",
			"  ${3:Text}",
			"  <icon name=\"${4:icon}\"/>",
			"</f-btn>",
			""
		],
		"description": "FBtn with icon"
	},
	"FBtn with left align": {
		"prefix": "fbtn-left-align",
		"body": [
			"<f-btn type=\"${1:primary}\" size=\"${2:large}\" leftAlign>",
			"  <div>${3:Text}</div>",
			"  <div>${4:Text}</div>",
			"</f-btn>",
			""
		],
		"description": "fbtn-left-align"
	},
	"FBtn with button wrapper": {
		"prefix": "fbtn-wrapper",
		"body": [
			"<f-btn-wrapper>",
			"  <f-btn class=\"mr-8\" type=\"primary\" size=\"large\">",
			"    ${1:Text}",
			"  </f-btn>",
			"  <f-btn type=\"primary\" size=\"large\">",
			"    ${2:Text}",
			"  </f-btn>",
			"</f-btn-wrapper>",
			""
		],
		"description": "FBtn with button wrapper"
	},
	"FCard default": {
		"prefix": "fcard",
		"body": [
			"<f-card>",
			"  <f-card-content>",
			"    ${1:Text}",
			"  </f-card-content>",
			"  <f-card-actions>",
			"    <f-btn type=\"secondary\">",
			"      ${2:Text}",
			"    </f-btn>",
			"    <f-btn type=\"primary\">",
			"       ${3:Text}",
			"    </f-btn>",
			"  </f-card-actions>",
			"</f-card>",
			"",
		],
		"description": "FCard default"
	},
	"FCard with button stacked": {
		"prefix": "fcard-btn-stacked",
		"body": [
			"<f-card>",
			"  <f-card-content>",
			"    ${1:Text}",
			"  </f-card-content>",
			"  <f-card-actions stacked>",
			"    <f-btn type=\"secondary\">",
			"      ${2:Text}",
			"    </f-btn>",
			"    <f-btn type=\"primary\">",
			"      ${3:Text}",
			"    </f-btn>",
			"  </f-card-actions>",
			"</f-card>",
			""
		],
		"description": "FCard with button stacked"
	},
	"FCell default": {
		"prefix": "fcell",
		"body": [
			"<f-cell class=\"pl-16 pr-4 py-12\">",
			"  <f-cell-icon class=\"mr-12\">",
			"    <icon name=\"${1:icon}\"/>",
			"  </f-cell-icon>",
			"  <f-cell-content>",
			"    <f-cell-label>",
			"      ${2:Label}",
			"    </f-cell-label>",
			"    <f-cell-desc>",
			"      ${3:Description}",
			"    </f-cell-desc>",
			"  </f-cell-content>",
			"</f-cell>",
			"<f-divider />",
			""
		],
		"description": "FCell default"
	},
	"FCell with arrow icon": {
		"prefix": "fcell-icon-arrow",
		"body": [
			"<f-cell class=\"pl-16 pr-4 py-12\">",
			"  <f-cell-icon class=\"mr-12\">",
			"    <icon name=\"${1:icon}\"/>",
			"  </f-cell-icon>",
			"  <f-cell-content>",
			"    <f-cell-label>",
			"      ${2:Label}",
			"    </f-cell-label>",
			"    <f-cell-desc>",
			"      ${3:Description}",
			"    </f-cell-desc>",
			"  </f-cell-content>",
			"  <f-cell-action class=\"f-flex flex-center\">",
			"    <div class=\"arrow\" />",
			"  </f-cell-action>",
			"</f-cell>",
			"<f-divider />",
			""
		],
		"description": "FCell with arrow icon"
	},
	"FCheckbox default": {
		"prefix": "fcheckbox",
		"body": [
			"<f-checkbox label=\"${1:Label}\" v-model=\"${2:data}\"></f-checkbox>",
			""
		],
		"description": "FCheckbox default"
	},
	"FDialog default": {
		"prefix": "fdialog",
		"body": [
			"<f-dialog type=\"modal\" v-model=\"${1:triggerModal}\">",
			"  <f-card class=\"p-12 pt-16\">",
			"    <f-card-content class=\"f-text-center\">",
			"      <f-card-img>",
			"        <icon name=\"${2:icon}\"/>",
			"      </f-card-img>",
			"      <div class=\"f-title title-16 my-8\">",
			"        ${3:Title}",
			"      </div>",
			"      <div class=\"f-caption caption-12\">",
			"        ${4:Desc}",
			"      </div>",
			"    </f-card-content>",
			"    <f-card-actions class=\"mt-16\" >",
			"      <f-btn type=\"ghost\" size=\"small\">",
			"        ${5:BUTTON}",
			"      </f-btn>",
			"      <f-btn type=\"primary\" size=\"small\">",
			"        ${6:BUTTON}",
			"      </f-btn>",
			"    </f-card-actions>",
			"  </f-card>",
			"</f-dialog>",
			""
		],
		"description": "FDialog default"
	},
	"FDialog with button stacked": {
		"prefix": "fdialog-btn-stacked",
		"body": [
			"<f-dialog type=\"modal\" v-model=\"${1:triggerModal}\">",
			"  <f-card class=\"p-12 pt-16\">",
			"    <f-card-content class=\"f-text-center\">",
			"      <f-card-img>",
			"        <icon name=\"${2:icon}\"/>",

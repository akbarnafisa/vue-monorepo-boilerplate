	"FSheetSwipe default": {
		"prefix": "fsheetswipe",
		"body": [
			"<f-sheet-swipe v-model=\"${1:triggerSheet}\">",
			"  <f-card rounded-top>",
			"    <div class=\"dash\" />",
			"    <f-card-content class=\"p-16\">",
			"      ${2:Text}",
			"    </f-card-content>",
			"    <f-card-actions class=\"px-16 pb-24\">",
			"      <f-btn type=\"secondary\">",
			"        ${3:BUTTON}",
